{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "baseUrl": "./",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2022",
    ],
  },
  "types": ["react", "jest"],
  "paths": {
    "*": [
      "node_modules/*",
    ],
    "Containers/*": ["src/containers/*"],
  },
  "include": [
    "src/**/*"
  ]
}